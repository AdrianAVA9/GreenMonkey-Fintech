@model ICollection<GreenMonkey.UI.Models.Suscriptor>

<p>
    @Html.ActionLink("Registrar suscriptor", "SuscriptorForm", null, new { @class="btn btn-primary pull-right" } )
</p>
<table class="table table-hover table-sm" style="margin-top:70px;">
    <tr class="table-primary">
        <th>
            Código
        </th>
        <th>
            Nombre
        </th>
        <th>
            Descripción
        </th>
        <th>
            Propietario
        </th>
        <th>
            Url
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Owner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BaseUrl)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { code =item.Code }) |
            @Html.ActionLink("Detalles", "Details", new { code = item.Code }) |
            @Html.ActionLink("Eliminar", "Delete", new { code = item.Code })
        </td>
    </tr>
}

</table>
